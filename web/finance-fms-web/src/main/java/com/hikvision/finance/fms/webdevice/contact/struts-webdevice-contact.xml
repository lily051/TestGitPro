<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="contactAction" namespace="/webdevice/contact" extends="ivms">
		<action name="*" method="{1}" class="contactAction">
		</action>
		<!-- 点击录单打开页面 -->
		<action name="toOpenPageSelectOrder" class="contactAction">
			<result>/web/webDevice/business/dialogs/selectOrder.jsp</result>
		</action>
		<!-- 打开投诉单历史页面 -->
		<action name="toOpenPageComplainOrder" class="contactAction">
			<result>/web/webDevice/business/dialogs/complainOrder.jsp</result>
		</action>
		<!-- 打开业务关联页面 -->
		<action name="toOpenPageBusiRelate" class="contactAction">
			<result>/web/webDevice/business/busiRelate.jsp</result>
		</action>
	</package>
</struts>