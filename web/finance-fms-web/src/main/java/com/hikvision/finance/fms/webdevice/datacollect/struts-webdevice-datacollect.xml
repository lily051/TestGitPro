<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="datacollectAction" namespace="/webdevice/datacollect" extends="ivms">
		<action name="*" method="{1}" class="datacollectAction">
		</action>
		<!-- 跳转到选择IPC通道页面 -->
		<action name="openChannelPage" class="recordAction">
			<result>/web/webDevice/datacollect/dialogs/selectChannel.jsp</result>
		</action>
		<!-- 跳转到选择USB通道页面 -->
		<action name="toOpenPageSelectUsb" class="recordAction">
			<result>/web/webDevice/datacollect/dialogs/selectUSB.jsp</result>
		</action>
		<!-- 跳转到编辑录像名 页面 -->
		<action name="toOpenPageEditVideoName" class="recordAction">
			<result>/web/webDevice/datacollect/dialogs/editVideoName.jsp</result>
		</action>
		<!-- 跳转到上传本地文件页面 -->
		<action name="toOpenPageLocalRecord" class="recordAction">
			<result>/web/webDevice/datacollect/dialogs/upload.jsp</result>
		</action>	
	</package>
</struts>