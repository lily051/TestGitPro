<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- 加密存储在本地文件中的数据库明文口令 (Patch v1.0开始支持) 文件路径以所在WEB-INF/classes目录开始  -->
	<securityproperties>
		<!-- 如果数据库明文口令位于properties中时，此段配置可以将其加密 -->
		<securityproperty filePath ="/WEB-INF/classes/system.properties" splitter ="=" >
			<encrypt-key>hibernate.connection.password</encrypt-key>
		</securityproperty>
	</securityproperties>
</root>